  "version": "3.0.0",
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native",
    "ios",
    "android",
    "drawer",
    "swipe"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-navigation/react-navigation.git",
    "directory": "packages/react-native-drawer-layout"
  },
  "bugs": {
    "url": "https://github.com/react-navigation/react-navigation/issues"
  },
