    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-pager-view": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
