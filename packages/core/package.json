  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "bob build",
    "clean": "del lib"
  },
  "dependencies": {
    "@react-navigation/routers": "^5.7.4",
    "escape-string-regexp": "^4.0.0",
    "nanoid": "^3.1.15",
    "query-string": "^6.13.6",
    "react-is": "^16.13.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^7.1.0",
    "@types/react": "^16.9.53",
    "@types/react-is": "^16.7.1",
    "del-cli": "^3.0.1",
    "react": "~16.13.1",
    "react-native-builder-bob": "^0.17.0",
    "react-test-renderer": "~16.13.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
